package com.sisyphus;

public class TestCase {

City city = new City("Austin");
	
	public String TestCityGrowth(long CityPop)
	{
		long growth = city.grow(GlobalConstants.RATE, GlobalConstants.MAX);
		return "with a population of "+CityPop+" will grow at by"+growth;
	}
	public String TestCityResc(long CityRes)
	{
		long newRes = city.upKeep(GlobalConstants.RATE);
		return CityRes+" resources will have "+newRes+" more/less resources";
	}

}
